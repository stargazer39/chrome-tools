{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["tabs", "bookmarks", "*://localhost:*/*"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "worker.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["contentStyles.css"],
        "js": ["contentScript.js"]
      }
    ]
  }
  